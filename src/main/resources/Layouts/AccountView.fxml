<?xml version="1.0" encoding="UTF-8"?>

<!--suppress XmlHighlighting -->

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <StackPane VBox.vgrow="ALWAYS">
        <HBox alignment="CENTER">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Account">
                <font>
                    <Font name="Arial" size="45.0" />
                </font>
            </Text>
        </HBox>
        <HBox alignment="TOP_RIGHT" spacing="10.0">
            <Button fx:id="editBtn" mnemonicParsing="false" onMouseClicked="#edit" text="Edit Account">
                <font>
                    <Font name="Arial" size="12.0" />
                </font>
            </Button>
        </HBox>
    </StackPane>
     <GridPane hgap="30.0" vgap="10.0">
         <columnConstraints>
             <ColumnConstraints hgrow="NEVER" />
             <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
         </rowConstraints>
         <Label text="First Name">
             <font>
                 <Font name="System Bold" size="24.0" />
             </font>
         </Label>
         <Label text="Last Name" GridPane.rowIndex="1">
             <font>
                 <Font name="System Bold" size="24.0" />
             </font>
         </Label>
         <Label text="Email" GridPane.rowIndex="2">
             <font>
                 <Font name="System Bold" size="24.0" />
             </font>
         </Label>
         <Text fx:id="firstName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1">
             <font>
                 <Font size="24.0" />
             </font>
         </Text>
         <Text fx:id="lastName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="1">
             <font>
                 <Font size="24.0" />
             </font>
         </Text>
         <Text fx:id="email" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="2">
             <font>
                 <Font size="24.0" />
             </font>
         </Text>
     </GridPane>
</fx:root>
