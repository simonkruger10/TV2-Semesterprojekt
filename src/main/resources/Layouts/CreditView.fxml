<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root hgap="30.0" type="GridPane" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="NEVER" />
      <ColumnConstraints hgrow="SOMETIMES" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label text="First Name" GridPane.rowIndex="1">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label text="Middle Name" GridPane.rowIndex="2">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label text="Last Name" GridPane.rowIndex="3">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label text="Credited for" GridPane.rowIndex="4">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Text fx:id="firstName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fx:id="middleName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fx:id="lastName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fx:id="groupName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <HBox alignment="TOP_RIGHT" spacing="10.0" GridPane.columnIndex="1">
         <children>
            <Button fx:id="addCreditsBtn" mnemonicParsing="false" onMouseClicked="#addCredits" text="Add Credit">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Button>
            <Button fx:id="editCreditsBtn" disable="true" mnemonicParsing="false" onMouseClicked="#editCredits" text="Edit Credit">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</fx:root>
