<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<!--suppress XmlHighlighting -->

<fx:root type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="TOP_RIGHT">
        <Button fx:id="editCreditBtn" disable="true" mnemonicParsing="false" onMouseClicked="#editCredit" text="Edit Credit">
            <font>
                <Font name="Arial" size="12.0" />
            </font>
        </Button>
    </HBox>
    <HBox fillHeight="false" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
        <ImageView fx:id="image" fitHeight="337.0" fitWidth="225.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets />
         </HBox.margin></ImageView>
        <GridPane hgap="30.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" />
                <ColumnConstraints hgrow="SOMETIMES" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
            </rowConstraints>
            <Label text="First Name">
                <font>
                    <Font name="System Bold" size="15.0" />
                </font>
            </Label>
            <Label text="Last Name" GridPane.rowIndex="1">
                <font>
                    <Font name="System Bold" size="15.0" />
                </font>
            </Label>
            <Label text="Credited for" GridPane.rowIndex="2">
                <font>
                    <Font name="System Bold" size="15.0" />
                </font>
            </Label>
            <Text fx:id="firstName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1">
                <font>
                    <Font size="15.0" />
                </font>
            </Text>
            <Text fx:id="lastName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <font>
                    <Font size="15.0" />
                </font>
            </Text>
            <Text fx:id="groupName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="2">
                <font>
                    <Font size="15.0" />
                </font>
            </Text>
        </GridPane>
    </HBox>
</fx:root>
