<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER_RIGHT" VBox.vgrow="ALWAYS">
        <padding>
            <Insets bottom="10.0"/>
        </padding>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sorter efter:">
            <font>
                <Font name="Arial" size="14.0"/>
            </font>
            <HBox.margin>
                <Insets right="10.0"/>
            </HBox.margin>
        </Text>
        <ComboBox fx:id="sortByBtn" disable="true" promptText="Navn (Alfabetisk)"/>
    </HBox>
    <javafx.scene.layout.VBox fx:id="main"/>
    <HBox alignment="CENTER" spacing="15.0" VBox.vgrow="ALWAYS">
        <padding>
            <Insets top="10.0"/>
        </padding>
        <Button fx:id="prvBtn" disable="true" onMouseClicked="#onPrv" text="Previous"/>
        <Button fx:id="nextBtn" onMouseClicked="#onNext" text="Next"/>
    </HBox>
</fx:root>
