<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<!--suppress XmlHighlighting, XmlHighlighting -->
<fx:root type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER_RIGHT" VBox.vgrow="ALWAYS">
        <padding>
            <Insets bottom="10.0" />
        </padding>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sorter efter">
            <font>
                <Font name="Arial" size="14.0" />
            </font>
            <HBox.margin>
                <Insets right="10.0" />
            </HBox.margin>
        </Text>
        <ComboBox fx:id="sortByBtn" promptText="Navn (Alfabetisk)" />
    </HBox>
    <VBox fx:id="main" />
    <HBox alignment="CENTER" spacing="15.0" VBox.vgrow="ALWAYS">
        <padding>
            <Insets top="5.0" />
        </padding>
        <Button fx:id="prvBtn" maxWidth="75.0" minWidth="75.0" onMouseClicked="#onPrv" text="&lt; Previous" managed="false" visible="false" />
        <Button fx:id="slot1" onMouseClicked="#onGoto" text="1" managed="false" visible="false" />
        <Button fx:id="slot2" onMouseClicked="#onGoto" text="2" managed="false" visible="false" />
        <Button fx:id="slot3" onMouseClicked="#onGoto" text="3" managed="false" visible="false" />
        <Button fx:id="slot4" onMouseClicked="#onGoto" text="4" managed="false" visible="false" />
        <Button fx:id="slot5" onMouseClicked="#onGoto" text="5" managed="false" visible="false" />
        <Button fx:id="slot6" onMouseClicked="#onGoto" text="6" managed="false" visible="false" />
        <Button fx:id="slot7" onMouseClicked="#onGoto" text="7" managed="false" visible="false" />
        <Button fx:id="nextBtn" maxWidth="75.0" minWidth="75.0" onMouseClicked="#onNext" text="Next &gt;" managed="false" visible="false" />
    </HBox>
</fx:root>
