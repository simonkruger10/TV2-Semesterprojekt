<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="720.0" prefWidth="1280.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <HBox alignment="CENTER_LEFT" style="-fx-background-color: #D21E1E;">
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
      <ImageView fx:id="homeBtn" fitHeight="50.0" onMouseClicked="#goHome" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/TV_2_Hvid_RGB.png" />
         </image>
      </ImageView>
      <HBox alignment="CENTER_RIGHT" spacing="10.0" HBox.hgrow="ALWAYS">
         <Text fx:id="helloMessage" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Hi, Guest">
            <font>
               <Font name="Arial" size="12.0" />
            </font>
         </Text>
         <Text fx:id="loginBtn" fill="#1690f0" onMouseClicked="#login" strokeType="OUTSIDE" strokeWidth="0.0" text="Log out" underline="true">
            <font>
               <Font name="Arial" size="12.0" />
            </font>
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
         </Text>
         <TextField fx:id="searchBarField" disable="true" onKeyPressed="#search" promptText="Search..." />
         <padding>
            <Insets right="5.0" />
         </padding>
      </HBox>
   </HBox>
   <HBox fx:id="contentHolder" fillHeight="false" spacing="40.0" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
      </VBox.margin>
      <VBox alignment="CENTER" minWidth="100.0" prefWidth="100.0" spacing="20.0">
         <Button fx:id="productionsBtn" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onMouseClicked="#showProductions" style="-fx-background-color: linear-gradient(#ff5400, #be1d00)linear-gradient(#ff5400, #be1d00);" text="Productions" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button fx:id="creditsBtn" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onMouseClicked="#showCredits" style="-fx-background-color: linear-gradient(#ff5400, #be1d00)linear-gradient(#ff5400, #be1d00);" text="Credits" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button fx:id="producersBtn" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onMouseClicked="#showProducers" style="-fx-background-color: linear-gradient(#ff5400, #be1d00)linear-gradient(#ff5400, #be1d00);" text="Producers" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button fx:id="accountsBtn" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onMouseClicked="#showAccounts" style="-fx-background-color: linear-gradient(#ff5400, #be1d00)linear-gradient(#ff5400, #be1d00);" text="Accounts" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button fx:id="accountBtn" maxWidth="1.7976931348623157E308" minHeight="40.0" mnemonicParsing="false" onMouseClicked="#showAccount" style="-fx-background-color: linear-gradient(#ff5400, #be1d00)linear-gradient(#ff5400, #be1d00);" text="Account" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
      </VBox>
      <ScrollPane fx:id="scrollBar" fitToHeight="true" fitToWidth="true" style="-fx-background-color: transparent;" HBox.hgrow="ALWAYS">
         <content>
            <VBox fx:id="content">
               <children>
                  <VBox fx:id="defaultContent" alignment="CENTER">
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Recently added credits">
                        <font>
                           <Font name="Arial" size="25.0" />
                        </font>
                        <VBox.margin>
                           <Insets bottom="30.0" />
                        </VBox.margin>
                     </Text>
                     <HBox fx:id="latestAddedSlider" alignment="CENTER" spacing="20.0">
                        <padding>
                           <Insets left="20.0" right="20.0" />
                        </padding>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                     </HBox>
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Credits for review">
                        <VBox.margin>
                           <Insets bottom="30.0" top="30.0" />
                        </VBox.margin>
                        <font>
                           <Font name="Arial" size="25.0" />
                        </font>
                     </Text>
                     <HBox fx:id="latestReviewSlider" alignment="CENTER" spacing="20.0">
                        <padding>
                           <Insets left="20.0" right="20.0" />
                        </padding>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                        <VBox alignment="CENTER">
                           <ImageView fitHeight="141.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@../images/TV_2_RGB.png" />
                              </image>
                           </ImageView>
                           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="asdasd">
                              <font>
                                 <Font name="Arial Bold Italic" size="24.0" />
                              </font>
                           </Text>
                        </VBox>
                     </HBox>
                  </VBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </HBox>
   <HBox id="HBox" alignment="CENTER" spacing="5.0" style="-fx-background-color: #D21E1E;" VBox.vgrow="NEVER">
      <padding>
         <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
      <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="TV 2 2021© - Alle rettigheder forbeholdes" textFill="WHITE" HBox.hgrow="ALWAYS">
         <font>
            <Font name="Arial Bold Italic" size="12.0" />
         </font>
      </Label>
   </HBox>
</fx:root>
